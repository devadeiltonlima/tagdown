{
  "version": 2,
  "builds": [
    {
      "src": "tagdown-backend/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "tagdown-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "tagdown-backend/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
